{
  "name": "@rickzhou/react-fabric",
  "version": "1.2.0",
  "description": "eslint / prettier config for React",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "author": "rick zhou <944268618@qq.com>",
  "homepage": "https://github.com/rick-chou/one-piece-react#readme",
  "keywords": [
    "eslint",
    "prettier",
    "config",
    "styleguide",
    "React"
  ],
  "contributors": [
    "rick zhou <944268618@qq.com>"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.yarnpkg.com/"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rick-chou/one-piece-react.git"
  },
  "bugs": {
    "url": "https://github.com/rick-chou/one-piece-react/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.56.0",
    "eslint-define-config": "^1.17.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.6",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.56.0"
  },
  "peerDependenciesMeta": {
    "eslint-config-xo": {
      "optional": true
    },
    "eslint-config-xo-typescript": {
      "optional": false
    }
  }
}
